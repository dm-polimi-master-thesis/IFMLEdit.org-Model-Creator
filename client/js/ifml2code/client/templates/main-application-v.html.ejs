<nav class="navbar navbar-default">
    <div class="container">
        <div class="navbar-header">
            <button class="navbar-toggle collapsed" 
                type="button" 
                data-toggle="collapse" 
                data-target="#landmark-menu" 
                aria-expanded="false">
                <span class="sr-only">
                    Toggle navigation
                </span>
                <span class="icon-bar">
                </span>
                <span class="icon-bar">
                </span>
                <span class="icon-bar">
                </span>
            </button>
        </div>
        <div id="landmark-menu" 
            class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
<% for (var i = 0; i < landmarks.length; i += 1) { -%>
                <li data-bind="click: landmark.bind($data,'<%= landmarks[i].id %>'), css: {active: active() === '<%= landmarks[i].id %>'}">
                    <a>
                        <%= landmarks[i].name %>
                    </a>
                </li>
<% } -%>
            </ul>
        </div>
    </div>
</nav>
<div class="container">
    <div class="row">
<% for (var i = 0; i < children.length; i += 1) { -%>
        <span data-bind="if: active() === '<%= children[i] %>'">
            <c-<%= children[i] %> params="context: context" 
                class="container">
            </c-<%= children[i] %>>
        </span>
<% } -%>
    </div>
</div>
