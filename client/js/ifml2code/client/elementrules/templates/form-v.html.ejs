<h3>
    <%=name %>
</h3>
<form>
<% for (var i = 0; i < fields.length; i += 1) { -%>
    <div class="form-group"
        data-bind="css: {'has-error': errors()['<%=fields[i].label%>']}">
        <label
            for="<%=id%>_field_<%=i%>"
            data-bind="css: {active: fields()['<%=fields[i].label%>']}, attr: {'data-error': errors()['<%=fields[i].label%>']}"
            class="control-label">
            <%=fields[i].label%>
        </label>
        <input id="<%=id%>_field_<%=i%>"
            data-bind="value: fields()['<%=fields[i].value%>']"
            type="<%=fields[i].type%>"
            <% if (fields[i].type === 'radio' || fields[i].type === 'checkbox') { -%>
              name="<%=fields[i].name%>"
            <% } -%>
            class="form-control validate"
            aria-describedby="<%=id%>_field_<%=i%>_error">
        <span id="<%=id%>_field_<%=i%>_error"
            class="help-block"
            data-bind="text: errors()['<%=fields[i].label%>']">
        </span>
    </div>
<% } -%>
</form>
<% if (events.length) { -%>
<div class="row">
<%     for (var i = 0; i < events.length; i += 1) {
           if (events[i].stereotype == 'system') { -%>
    <c-<%= events[i].id %> params="trigger: trigger.bind($data,'<%= events[i].id %>')"></c-<%= events[i].id %>>
<%         } else { -%>
    <a class="col-xs-2 btn btn-primary"
        data-bind="click: trigger.bind($data, '<%= events[i].id %>')">
        <%= events[i].name %>
    </a>
<%         }
       } -%>
</div>
<% } -%>
