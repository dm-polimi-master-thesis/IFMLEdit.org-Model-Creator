<h3><%=name %></h3>
<table class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>
                #
            </th>
<% for (var i = 0; i < fields.length; i += 1) { -%>
            <th>
                <%= fields[i]%>
            </th>
<% }
   if (events.length) { -%>
            <th>
                actions
            </th>
<% } -%>
        </tr>
    </thead>
    <tbody data-bind="foreach: items">
<% if (showSelection) { -%>
        <tr data-bind="click: $parent.select, css: {'info': $parent.selected() === id}">
<% } else { -%>
        <tr data-bind="click: $parent.select">
<% } -%>
            <td data-bind="text: id">
            </td>
<%   for (var i = 0; i < fields.length; i += 1) { -%>
            <td data-bind="text: $data['<%= fields[i] %>']"
                style="white-space: pre-wrap;">
            </td>
<%
   }
   if (events.length) { -%>
            <td>
<%     for (var i = 0; i < events.length; i += 1) {
           if (events[i].stereotype == 'system') { -%>
                <c-<%= events[i].id %> params="trigger: $parent.trigger.bind($data, '<%= events[i].id %>')"></c-<%= events[i].id %>>
<%         } else { -%>
                <a class="col-xs-2 btn btn-primary"
                    data-bind="click: $parent.trigger.bind($data, '<%= events[i].id %>'), clickBubble: false">
                    <%= events[i].name %>
                </a>
<%         }
       } -%>
            </td>
<% } -%>
        </tr>
    </tbody>
</table>
